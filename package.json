{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with VanilaJS and NodeJS",
  "main": "index.js",
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore 'scss'",
    "dev:assets": "WEBPACK_ENV=development webpack -w",
    "build:assets": "WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets', 'src/static'",
    "build": "npm run build:server && npm run build:assets",
    "prebuild": "rm -rf build",
    "tunnel": "lt --port 4000",
    "start": "node build/init.js",
    "prestart": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coseo12/wetube.git"
  },
  "keywords": [],
  "author": "C.O.SEO",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coseo12/wetube/issues"
  },
  "homepage": "https://github.com/coseo12/wetube#readme",
  "dependencies": {
    "aws-sdk": "^2.830.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "get-blob-duration": "^1.1.2",
    "helmet": "^4.3.1",
    "mongoose": "^5.11.12",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "pug": "^3.0.0",
    "postcss-loader": "^4.1.0",
    "sass": "^1.32.4",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "autoprefixer": "^10.2.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.1",
    "mini-css-extract-plugin": "^1.3.4"
  },
  "devDependencies": {
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7"
  },
  "browserslist": [
    "cover 99.5%"
  ]
}
